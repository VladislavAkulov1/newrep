package zadanie3;

public class zadanie3 {
	public static void main(String[] args) {
        int sum1 = 0;
        int cyfry=0;
        for(int i = 5;; i += 6) {
            sum1 += i;
            cyfry++;
            if(cyfry == 15) {
                break;
            }
        }
        System.err.println(sum1);
    }

}
